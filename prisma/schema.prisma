generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Item {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  image     String
  price     Float
  isAvailable Boolean
  link      String
}

enum ItemStatus {
  REQUIRES_FETCH
  UPDATED
}